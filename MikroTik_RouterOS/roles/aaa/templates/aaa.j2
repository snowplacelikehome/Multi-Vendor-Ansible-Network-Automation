/radius remove [find where service=login]
{% for aaa_serv in aaa_servers %}
/radius add address={{ aaa_serv.ipv4 }} authentication-port={{ radius_auth_port }} accounting-port={{ radius_acct_port }} comment="{{ aaa_serv.server_name }}" service=login src-address={{ aaa_source_ip }} secret="{{ aaa_secret }}"
/user aaa set use-radius=yes
{% endfor %}